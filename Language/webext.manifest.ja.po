msgctxt "name"
msgid "bili Browser Extension"
msgstr "biliブラウザ拡張機能"

msgctxt "desc"
msgid ""
"Use bili URI scheme to start bili command line program.\n"
"Source code: https://github.com/lifegpc/biliwebext"
msgstr ""
"bili URI スキームを使って bili コマンドラインプログラムを起動します。\n"
"ソースコード: https://github.com/lifegpc/biliwebext"

msgctxt "open"
msgid "Use bili to open this page"
msgstr "biliを使用してこのページを開く"

msgctxt "link"
msgid "Use bili to open this link"
msgstr "biliを使用してこのリンクを開く"

msgctxt "sel"
msgid "Use bili to open selected text"
msgstr "biliを使用して選択したテキストを開く"

msgctxt "newtab"
msgid "Open this page in new tab."
msgstr "新しいタブでこのページを開く"

msgctxt "inputc"
msgid "Input: "
msgstr "入力: "

msgctxt "open2"
msgid "Open bili"
msgstr "biliを開く"

msgctxt "downmethod"
msgid "Download method: "
msgstr "ダウンロード方法: "

msgctxt "partnumber"
msgid "The number of videos/parts you want to download: "
msgstr "ダウンロードしたい動画/パートの数: "

msgctxt "endownmaxq"
msgid "Enable download with maximum quality."
msgstr "最高品質でのダウンロードを有効にする。"

msgctxt "didownmaxq"
msgid "Disable download with maximum quality."
msgstr "最高画質でのダウンロードを無効にする。"

msgctxt "sett"
msgid "Settings Page"
msgstr "設定画面"

msgctxt "savesett"
msgid "Save"
msgstr "保存"

msgctxt "saveok"
msgid "Save successfully."
msgstr "保存成功"

msgctxt "withpa"
msgid " (with parameters)"
msgstr "(パラメータを含む)"

msgctxt "encodown"
msgid "Enable \"Continue Download\" function."
msgstr "「ダウンロードを続ける」機能を有効にする。"

msgctxt "dicodown"
msgid "Disable \"Continue Download\" function."
msgstr "「ダウンロードを続ける」機能を無効にする。"

msgctxt "enfilter"
msgid "Enable filter barrage."
msgstr "弾幕フィルターを有効にする。"

msgctxt "difilter"
msgid "Disable filter barrage."
msgstr "弾幕フィルターを無効にする。"

msgctxt "endelulf"
msgid "Enable delete useless files after merge is complete."
msgstr "マージ完了後に無駄なファイルを削除するのを有効にする。"

msgctxt "didelulf"
msgid "Disable delete useless files after merge is complete."
msgstr "マージ完了後に無駄なファイルを削除するのを無効にする。"

msgctxt "enredown"
msgid "Enable download again automatically after the download failed."
msgstr "ダウンロードに失敗した後、自動的に再ダウンロードを有効にする。"

msgctxt "diredown"
msgid "Disable download again automatically after the download failed."
msgstr "ダウンロードに失敗した後、自動的に再ダウンロードを無効にする。"

msgctxt "enusevd"
msgid "Put video/audio quality information in the file name."
msgstr ""

msgctxt "diusevd"
msgid "Do not put video/audio quality information in the file name."
msgstr ""

msgctxt "enaddmeta"
msgid ""
"Add video information to file forcibly. (This will transcode the video and "
"transcoding will not affect the video/audio quality.)"
msgstr ""

msgctxt "diaddmeta"
msgid "Do not add video information to file forcibly."
msgstr ""
